<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    

    <style name="title_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/rectangle_title_background</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
<!--         <item name="android:textStyle">bold</item> -->
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="setup_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textColor">@color/ablack</item>
        <item name="android:textSize">23sp</item>
        <item name="android:gravity">center</item>
        
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
   
    <style name="stup1_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
<!--         <item name="android:drawableStart">@android:drawable/star_big_on</item> -->
        <item name="android:textColor">@color/ablack</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
        
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="stup_context_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>

        <item name="android:textColor">@color/ablack</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center_vertical</item>
       
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="context_Left_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/ablack</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center_vertical</item>      
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:clickable">true</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="context_Right_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/ablack</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center_vertical</item>      
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="RelativeLayout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">5dp</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="view_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.1dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:background">@color/ablack</item>
    </style>
    
 
            
    

</resources>
